# Installs the acme/ohmyzsh module from the Puppet Forge
mod 'acme/ohmyzsh', '0.1.2'

# Installs the puppetlabs/vcsrepo module from its GitHub repository
mod 'vcsrepo',
  :git => 'https://github.com/puppetlabs/puppetlabs-vcsrepo'

# Installs the arioch redis module
mod 'arioch/redis'

# Install epel repository module
mod 'stahnma/epel'

# Install registry module from puppetlabs for windows
mod 'puppetlabs/registry'
# install slurm
mod 'chwilk-slurm'
